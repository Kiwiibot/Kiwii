{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "resolveJsonModule": true,
    "lib": [
      "DOM",
      "DOM.Iterable"
    ]
  },
  "include": [
    "src/**/*"
  ]
}